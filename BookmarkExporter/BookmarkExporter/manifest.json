{
  "manifest_version": 2,
  "name": "bookmarkExporter",
  "description": "Bookmark test extension for Opera 15",
  "version": "0.1",
  "developer": {
    "name": "Sergios Kefalas",
    "url": "https://github.com/serkef"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": ["bookmarks", "downloads"],
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Export bookmarks!"
  }
}